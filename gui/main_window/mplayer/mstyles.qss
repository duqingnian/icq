Ui--DialogPlayer
{
    background-color: #000000;
    border: none;
}

QWidget#control_panel_gradient[mode="dialog"]
{
    border-bottom-right-radius: 8dip;
    border-bottom-left-radius: 8dip;
    border-image: url(:/videoplayer/gradient_a_100);
}

QWidget#control_panel_gradient[mode="window"]
{
    border-image: url(:/videoplayer/gradient_a_100);
    border-radius: 0;
}


QWidget#control_panel_gradient[mode="full_screen"]
{
    border-image: url(:/videoplayer/gradient_b_100);
    border-radius: 0;
}


